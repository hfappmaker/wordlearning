<?xml version="1.0" encoding="UTF-8" ?>
<LinearLayout
xmlns:android="http://schemas.android.com/apk/res/android"
android:id="@+id/ll_Dialog_SetLearnWord"
android:layout_width="match_parent"
android:layout_height="wrap_content"
android:orientation="vertical">
<LinearLayout
android:layout_width="match_parent"
android:layout_height="wrap_content"
android:orientation="horizontal">
        <android.support.design.widget.TextInputLayout
        android:id="@+id/_tlnInterval_Dialog_SettingWordlist"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="bottom">
            <android.support.design.widget.TextInputEditText
            android:layout_width="match_parent"
            android:hint="@string/Interval_Dialog_SettingWordlist"
            android:layout_height="48dp"
            android:id="@+id/etxtInterval_Dialog_SettingWordlist"
            android:inputType="number"
            android:scrollbars="vertical"/>
        </android.support.design.widget.TextInputLayout>
</LinearLayout>
<TextView
android:layout_width="match_parent"
android:id = "@+id/tvChooselanguageWord_Dialog_SettingWordlist"
android:layout_height="50dp"
android:text = "@string/ChooselanguageWord_Dialog_SettingWordlist"
android:padding="10dp"
android:textColor="@color/material_deep_teal_500"
android:textAppearance="?android:attr/textAppearanceMedium">       
</TextView>
<Spinner
android:id="@+id/spChooselanguageWord_Dialog_SettingWordlist"
android:layout_width="wrap_content"
android:layout_height="wrap_content"/>
<TextView
android:layout_width="match_parent"
android:id = "@+id/tvChooselanguageMeaning_Dialog_SettingWordlist"
android:layout_height="50dp"
android:text = "@string/ChooselanguageMeaning_Dialog_SettingWordlist"
android:padding="10dp"
android:textColor="@color/material_deep_teal_500"
android:textAppearance="?android:attr/textAppearanceMedium">       
</TextView>
<Spinner
android:id="@+id/spChooselanguageMeaning_Dialog_SettingWordlist"
android:layout_width="wrap_content"
android:layout_height="wrap_content"/>
</LinearLayout>
